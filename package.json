{
  "name": "@ctrl/react-adsense",
  "version": "0.0.0-placeholder",
  "description": "Typescript adsense module",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/react-adsense",
  "sideEffects": false,
  "keywords": [
    "typescript"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "pika build",
    "test": "jest",
    "test:update": "jest --update-snapshot",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage",
    "semantic-release": "cd pkg && semantic-release"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "tsconfig": "tsconfig.build.json"
        }
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-bundle-web"
      ]
    ]
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@pika/pack": "0.5.0",
    "@pika/plugin-build-web": "0.9.2",
    "@pika/plugin-bundle-web": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "0.9.2",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.1",
    "@types/jest": "^25.2.3",
    "@types/node": "14.0.11",
    "@types/react": "^16.9.35",
    "@typescript-eslint/eslint-plugin": "3.1.0",
    "@typescript-eslint/parser": "3.1.0",
    "eslint": "7.2.0",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-config-xo-space": "0.25.0",
    "eslint-config-xo-typescript": "0.31.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jest": "26.0.1",
    "jest-junit": "10.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "semantic-release": "17.0.8",
    "ts-jest": "26.1.0",
    "typescript": "3.9.5"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master"
  }
}
